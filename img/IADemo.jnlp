<?xml version="1.0" encoding="utf-8"?>
<!-- JNLP file for Ignition client launch -->
<jnlp
  spec="1.0+"
  codebase="http://iademo.inductiveautomation.com:80/main/"
  href="system/launch/client/IADemo.jnlp">
  <information>
    <title>Ignition Demo</title>
    <vendor>Inductive Automation</vendor>
