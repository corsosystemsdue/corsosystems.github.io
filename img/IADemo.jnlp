    <icon href="web/platform_logo.png"/>
    <offline-allowed/>
    <shortcut online="true">
      <desktop/>
      <menu/>
    </shortcut>
  </information>
  <security><all-permissions/></security>
  <resources>
    <j2se version="1.8" initial-heap-size="32M" max-heap-size="256M" java-vm-args="-XX:MaxPermSize=128m"/>
